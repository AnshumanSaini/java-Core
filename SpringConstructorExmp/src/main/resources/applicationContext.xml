<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean id="add" class="com.hughes.spring.AdditionPOJO">
		<constructor-arg value="10" type="int"></constructor-arg>
		<constructor-arg value="20" type="int"></constructor-arg>
	</bean>
	<bean id="sub" class="com.hughes.spring.SubtractionPOJO">
		<constructor-arg value="10" type="int"></constructor-arg>
		<constructor-arg value="20" type="int"></constructor-arg>
	</bean>
	<bean id="constructor" class="com.hughes.setter.Customer">
		<property name="custName" value="Anshuman"></property>
		<property name="customerId" value="500"></property>
	</bean>

</beans>