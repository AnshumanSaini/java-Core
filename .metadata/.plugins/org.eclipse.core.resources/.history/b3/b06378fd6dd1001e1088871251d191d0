package com.hughes.test;


class A
{
	public void display()
	{
		System.out.println("A");
	}
}

class B extends A
{
	public void display()
	{
		System.out.println("B");
	}
}

class C extends A
{
	public void display()
	{
		System.out.println("C");
	}
}
public class HierInhTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		A a=new B();
		a.display();
		a=new C();
		a.display();

	}

}
