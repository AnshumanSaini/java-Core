package com.hughes.test;
import com.hughes.customExceptions.CharacterException;

public class Fourteen {
	public static void check(String str) throws NumberFormatException, CharacterException
	{
		try
		{
			int a=Integer.parseInt(str);
			System.out.println(Integer.parseInt(str));
			throw new NumberFormatException("this is a number");
		}
		catch(Exception e)
		{
			if(str.length()==1) throw new CharacterException("this is a Character");
			System.out.println(str);
		}
	}

	public static void main(String[] args)  {
		// TODO Auto-generated method stub
		try
		{
//			check("hey");
//			check("h");
			check("1");
			
		}
		catch(Exception e)
		{
			e.printStackTrace();
		}
	}

}
