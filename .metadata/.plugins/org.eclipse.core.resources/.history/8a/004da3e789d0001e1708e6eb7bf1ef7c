package com.hughes.test;

public class Seven {

	public static void main(String[] args) 
	{
//		String s="vasu34k@gmail.com";
		String s="bob34kgmail.com";
		int n=s.length();
		String chk=s.substring(n-10);
		if(chk.equals("@gmail.com")) System.out.println("Valid");
		else System.out.println("Invalid");
	}

}
