package com.hughes.test;

//write a java program for beow requirement.
//i/p:String a = "Computer"
//o/p: r e t u p m o C

public class Eight {

	public static void main(String[] args) {
		String s="Computer";
		
		for(int i=s.length()-1;i>=0;--i)
		{
			System.out.print(s.charAt(i)+" ");
		}

	}

}
